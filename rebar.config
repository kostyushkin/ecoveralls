{deps, [
  {jsx, ".*", {git, "git://github.com/talentdeficit/jsx", {tag, "v2.0.4"}}},
  {hackney, ".*", {git, "git://github.com/benoitc/hackney", {tag, "0.13.0"}}}
]}.

{erl_opts, [
  warn_bif_clash,
  warn_deprecated_function,
  warn_deprecated_type,
  warn_export_all,
  warn_shadow_vars,
  warn_obsolete_guard,
  warn_unused_import,
  warn_unused_function,
  warn_unused_record,
  warn_unused_vars,
  warnings_as_errors
]}.
